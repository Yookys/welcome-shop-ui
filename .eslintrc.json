{
  "env": {
    "browser": true,
    "jest/globals": true
  },
  "extends": ["airbnb-typescript", "prettier", "plugin:jsdoc/recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json",
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2015,
    "sourceType": "module"
  },
  "plugins": ["prettier", "react", "react-hooks", "@typescript-eslint", "import", "jest", "jsdoc"],
  "rules": {
    "require-jsdoc": [
      "error",
      {
        "require": {
          "FunctionDeclaration": true,
          "MethodDefinition": true,
          "ClassDeclaration": true,
          "ArrowFunctionExpression": true,
          "FunctionExpression": true
        }
      }
    ],
    "@typescript-eslint/default-param-last": "off",
    "no-plusplus": "off",
    "no-underscore-dangle": "off",
    "jsdoc/check-types": "error",
    "jsdoc/check-tag-names": "error",
    "jsdoc/check-param-names": "off",
    "jsdoc/require-property-description": "off",
    "jsdoc/require-param": ["error", {"checkRestProperty": true, "autoIncrementBase": 0, "checkDestructured": false}],
    "jsdoc/newline-after-description": "off",
    "jsdoc/require-param-type": "off",
    "jsdoc/require-returns": "off",
    "jest/no-disabled-tests": "warn",
    "jest/no-focused-tests": "error",
    "jest/no-identical-title": "error",
    "jest/prefer-to-have-length": "warn",
    "jest/valid-expect": "error",
    "react/no-array-index-key": "off",
    "@typescript-eslint/no-throw-literal": "off",
    "jsx-a11y/no-noninteractive-element-interactions": "off",
    "import/no-extraneous-dependencies": "off",
    "react/button-has-type": "off",
    "react/prop-types": "off",
    "consistent-return": "off",
    "react/require-default-props": "off",
    "react/jsx-props-no-spreading": "off",
    "react/destructuring-assignment": "off",
    "react/state-in-constructor": "off",
    "@typescript-eslint/no-redeclare": "off",
    "@typescript-eslint/lines-between-class-member": "off",
    "no-console": "off",
    "no-template-curly-in-string": "off",
    "jsx-a11y/click-events-have-key-events": "off",
    "jsx-a11y/no-static-element-interactions": "off",
    "no-bitwise": "off",
    "no-unneeded-ternary": "off",
    "import/prefer-default-export": "off",
    "prefer-destructuring": "off"
  },
  "settings": {
    "import/extensions": [".js", ".jsx", ".ts", ".tsx"],
    "import/parsers": {
      "@typescript-eslint/parser": [".ts", ".tsx"]
    },
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  }
}
