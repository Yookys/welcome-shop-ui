{
  "name": "cc-fe-bonus-options",
  "version": "1.3.0",
  "description": "VTB CC - Bonus options",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "lint": "npx eslint src/**",
    "test": "jest --silent",
    "test:coverage": "jest --coverage --silent",
    "test:watch": "jest --watch --coverage --silent",
    "test-snapshots-update": "jest --u --coverage --silent",
    "start": "webpack serve --config config/webpack.dev.config.ts",
    "start-docker": "docker run --env-file docker/.env -d -p 8080:8080 bonus-option-test:0.0.0-test",
    "build": "npm run lint && npm run test && webpack --config config/webpack.prod.config.ts && cd webserver && npm run build-web-server",
    "build-docker": "docker build -t bonus-option-test:0.0.0-test -f docker/Dockerfile .",
    "serve-docker": "npm run get-webserver && npm i husky -G && npm i && npm run build && npm run build-docker && npm run start-docker",
    "get-webserver": "getWebserver.bat"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "7.14.5",
    "@testing-library/react": "12.0.0",
