    "identity-obj-proxy": "3.0.0",
    "jest": "27.1.0",
    "jest-styled-components": "7.0.5",
    "jest-svg-transformer": "1.0.0",
    "jest-transform-stub": "2.0.0",
    "lodash": "4.17.21",
    "prettier": "2.3.0",
    "prop-types": "15.7.2",
