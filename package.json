  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test"
    }
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "./config/setupTest.ts"
    ],
    "transform": {
      "^.+\\.[jt]sx?$": "babel-jest"
    },
    "moduleNameMapper": {
      ".+\\.(styl|jpg|jpeg|png|gif|eot|otf|webp|svg|svgr|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|css|less|scss)$": "identity-obj-proxy",
      "^@common(.*)$": "<rootDir>/src/common$1",
      "^@assets(.*)$": "<rootDir>/src/common/assets$1",
      "^@modules(.*)$": "<rootDir>/src/modules$1",
      "^@Core(.*)$": "<rootDir>/src/modules/Core$1"
    }
  }
